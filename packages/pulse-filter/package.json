{
  "name": "@agent-pulse/middleware",
  "version": "1.0.0",
  "description": "Lightweight liveness filter for Agent Pulse â€” check if on-chain agents are alive using just fetch()",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:live": "PULSE_LIVE_TEST=1 vitest run",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "agent-pulse",
    "liveness",
    "pulse",
    "base",
    "ethereum",
    "middleware",
    "express",
    "langchain",
    "ai-agents",
    "filter",
    "on-chain"
  ],
  "author": "Agent Pulse Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/consensus-hq/agent-pulse.git",
    "directory": "packages/pulse-filter"
  },
  "homepage": "https://agentpulse.xyz",
  "bugs": {
    "url": "https://github.com/consensus-hq/agent-pulse/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
