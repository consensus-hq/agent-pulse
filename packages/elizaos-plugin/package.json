{
  "name": "@openclaw/eliza-plugin-pulse",
  "version": "0.1.0",
  "description": "ElizaOS plugin for Agent Pulse protocol with gate functionality - on-chain liveness signaling",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "^1.7.0",
    "viem": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@elizaos/core": "^1.7.0"
  },
  "keywords": [
    "elizaos",
    "plugin",
    "agent-pulse",
    "liveness",
    "base",
    "ethereum"
  ],
  "license": "MIT"
}